<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.examples.HAS_A_ManyToMany.Faculty" table="faculty">
        <id name="fid" >
            <generator class="increment"></generator>
        </id>
        <property name="fname"/>
        <set name="courses" table="faculty_course" cascade="all">
            <key column="fid"></key>
            <many-to-many class="org.examples.HAS_A_ManyToMany.Course" column="cid"></many-to-many>
        </set>

    </class>

</hibernate-mapping>